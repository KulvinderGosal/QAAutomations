{
  "name": "pushengage-qa-automation",
  "version": "1.0.0",
  "description": "Automated QA testing framework for PushEngage plugin using Playwright",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:single": "playwright test --project=chromium",
    "test:multi": "playwright test",
    "test:headed": "playwright test --headed",
    "test:headed:single": "playwright test --headed --project=chromium",
    "test:debug": "playwright test --debug",
    "test:smoke": "playwright test tests/pushengage-regression/critical/installation/",
    "test:smoke:single": "playwright test tests/pushengage-regression/critical/installation/ --project=chromium",
    "test:wordpress-plugin": "playwright test tests/pushengage-regression/critical/installation/02-plugin-smoke-test.spec.js",
    "test:wordpress-plugin:single": "playwright test tests/pushengage-regression/critical/installation/02-plugin-smoke-test.spec.js --project=chromium",
    "test:pushengage": "playwright test tests/pushengage/",
    "test:pushengage:single": "playwright test tests/pushengage/ --project=chromium",
    "test:broadcast": "playwright test tests/pushengage/broadcast-test.spec.js --project=chromium",
    "test:send-broadcast": "playwright test tests/pushengage/send-broadcast.spec.js --project=chromium",
    "test:auto-broadcast": "TEST_ENV=local playwright test tests/pushengage/auto-send-broadcast.spec.js --project=chromium",
    "test:auto-broadcast:headed": "TEST_ENV=local playwright test tests/pushengage/auto-send-broadcast.spec.js --headed --project=chromium",
    "test:manual-broadcast": "TEST_ENV=local playwright test tests/pushengage/manual-broadcast-sender.spec.js --headed --project=chromium",
    "test:local:send-broadcast": "TEST_ENV=local playwright test tests/pushengage/send-broadcast.spec.js --project=chromium",
    "test:local:send-broadcast:headed": "TEST_ENV=local playwright test tests/pushengage/send-broadcast.spec.js --headed --project=chromium",
    "test:local": "TEST_ENV=local playwright test --project=chromium",
    "test:local:smoke": "TEST_ENV=local playwright test tests/pushengage-regression/critical/installation/ --project=chromium",
    "test:local:pushengage": "TEST_ENV=local playwright test tests/pushengage/ --project=chromium",
    "test:local:broadcast": "TEST_ENV=local playwright test tests/pushengage/broadcast-test.spec.js --project=chromium",
    "test:local:headed": "TEST_ENV=local playwright test --headed --project=chromium",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "report": "playwright show-report",
    "codegen": "playwright codegen",
    "install:deps": "npm install",
    "generate:tests": "node generate-test-suite.js",
    "test:regression:critical": "TEST_ENV=local playwright test tests/pushengage-regression/critical/ --project=chromium",
    "test:regression:high": "TEST_ENV=local playwright test tests/pushengage-regression/high/ --project=chromium",
    "test:regression:medium": "TEST_ENV=local playwright test tests/pushengage-regression/medium/ --project=chromium",
    "test:regression:low": "TEST_ENV=local playwright test tests/pushengage-regression/low/ --project=chromium",
    "test:regression:all": "TEST_ENV=local playwright test tests/pushengage-regression/ --project=chromium",
    "test:regression:broadcasts": "TEST_ENV=local playwright test tests/pushengage-regression/critical/push-broadcasts/ --project=chromium",
    "test:regression:settings": "TEST_ENV=local playwright test tests/pushengage-regression/critical/settings-core/ --project=chromium",
    "test:regression:drip": "TEST_ENV=local playwright test tests/pushengage-regression/high/drip-campaigns/ --project=chromium",
    "test:regression:triggers": "TEST_ENV=local playwright test tests/pushengage-regression/high/triggers/ --project=chromium",
    "test:regression:woo": "TEST_ENV=local playwright test tests/pushengage-regression/high/woocommerce-core/ --project=chromium",
    "test:regression:all:headed": "TEST_ENV=local playwright test tests/pushengage-regression/ --headed --project=chromium",
    "test:regression:critical:headed": "TEST_ENV=local playwright test tests/pushengage-regression/critical/ --headed --project=chromium",
    "test:regression:installation": "TEST_ENV=local playwright test tests/pushengage-regression/critical/installation/ --project=chromium",
    "test:regression:onboarding": "TEST_ENV=local playwright test tests/pushengage-regression/critical/onboarding/ --project=chromium",
    "test:regression:dashboard": "TEST_ENV=local playwright test tests/pushengage-regression/critical/dashboard/ --project=chromium",
    "test:regression:campaigns": "TEST_ENV=local playwright test tests/pushengage-regression/critical/campaigns/ --project=chromium",
    "test:regression:settings-excel": "TEST_ENV=local playwright test tests/pushengage-regression/critical/settings-excel/ --project=chromium",
    "test:regression:audience": "TEST_ENV=local playwright test tests/pushengage-regression/high/audience/ --project=chromium",
    "test:regression:design": "TEST_ENV=local playwright test tests/pushengage-regression/medium/design/ --project=chromium",
    "test:regression:analytics": "TEST_ENV=local playwright test tests/pushengage-regression/medium/analytics/ --project=chromium",
    "convert:excel": "node convert-excel-to-tests.js",
    "test:signup": "playwright test tests/sign-up/",
    "test:signup:headed": "playwright test tests/sign-up/ --headed",
    "test:signup:debug": "playwright test tests/sign-up/ --debug",
    "test:app": "playwright test --config=playwright-app.config.js --project=chromium",
    "test:app:headed": "playwright test --config=playwright-app.config.js --headed --project=chromium",
    "test:app:debug": "playwright test --config=playwright-app.config.js --debug --project=chromium",
    "test:app:dashboard": "playwright test tests/pushengage-app/dashboard/ --config=playwright-app.config.js --project=chromium",
    "test:app:dashboard:headed": "playwright test tests/pushengage-app/dashboard/ --config=playwright-app.config.js --headed --project=chromium",
    "test:app:dashboard:debug": "playwright test tests/pushengage-app/dashboard/ --config=playwright-app.config.js --debug --project=chromium",
    "test:app:save-session": "node tests/pushengage-app/save-session.js",
    "playwright:install": "playwright install chromium firefox webkit",
    "playwright:install:deps": "playwright install-deps",
    "playwright:clean": "rm -rf ~/Library/Caches/ms-playwright/chromium-* ~/Library/Caches/ms-playwright/firefox-* ~/Library/Caches/ms-playwright/webkit-* && npm run playwright:install",
    "playwright:status": "playwright --version && du -sh ~/Library/Caches/ms-playwright/",
    "test:smoke": "playwright test tests/smoke/",
    "test:smoke:fast": "playwright test tests/smoke/critical-smoke.spec.js --project=chromium --workers=5",
    "test:smoke:headed": "playwright test tests/smoke/ --headed",
    "test:regression:fast": "playwright test tests/pushengage-regression/critical/ --project=chromium --workers=10 --retries=1",
    "test:regression:parallel": "playwright test tests/pushengage-regression/ --project=chromium --workers=20 --retries=1"
  },
  "keywords": [
    "playwright",
    "testing",
    "wordpress",
    "plugin",
    "qa",
    "automation",
    "regression-testing"
  ],
  "author": {
    "name": "Kulvinder Singh",
    "email": "kgosal@awesomemotive.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KulvinderGosal/QAAutomations.git"
  },
  "bugs": {
    "url": "https://github.com/KulvinderGosal/QAAutomations/issues",
    "email": "kgosal@awesomemotive.com"
  },
  "homepage": "https://github.com/KulvinderGosal/QAAutomations#readme",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "dotenv": "^16.0.0",
    "html-pdf-node": "^1.0.8",
    "pdfkit": "^0.17.2",
    "xlsx": "^0.18.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
